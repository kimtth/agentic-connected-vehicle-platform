# API Configuration - includes the /api path suffix that's used in the config.js file
REACT_APP_API_BASE_URL=

# Set NODE_ENV for development tools
NODE_ENV=production

# Enable React debugging
REACT_APP_DEBUG=true

# CORS settings
REACT_APP_ALLOW_CORS=true

# Optional: Authentication settings (uncomment if needed)
# REACT_APP_AZURE_CLIENT_ID=
# REACT_APP_AZURE_AUTHORITY=https://login.microsoftonline.com/your-tenant-id
# REACT_APP_AZURE_REDIRECT_URI=http://localhost:3000